install-DyLPDocInstallDATA: $(DyLPDocInstall_DATA)
	@$(NORMAL_INSTALL)
	@list='$(DyLPDocInstall_DATA)'; test -n "$(DyLPDocInstalldir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(DyLPDocInstalldir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(DyLPDocInstalldir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(DyLPDocInstalldir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(DyLPDocInstalldir)" || exit $$?; \
	done

uninstall-DyLPDocInstallDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(DyLPDocInstall_DATA)'; test -n "$(DyLPDocInstalldir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(DyLPDocInstalldir)'; $(am__uninstall_files_from_dir)
